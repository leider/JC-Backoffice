{
  "type": "module",
  "name": "jazzclub-backoffice",
  "description": "The back office for Jazzclub Karlsruhe",
  "version": "1.0.0",
  "author": "Andreas Leidig",
  "private": true,
  "workspaces": [
    "backend",
    "batchjobs",
    "vue",
    "shared",
    "rider"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/leider/JC-Backoffice"
  },
  "license": "(MIT OR Apache-2.0)",
  "main": "",
  "engines": {
    "node": ">=18.0"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@types/chai": "4.3.11",
    "@types/lodash": "4.14.202",
    "@types/mocha": "10.0.6",
    "@types/node": "20.10.2",
    "@types/sinon": "17.0.2",
    "@types/uuid": "9.0.7",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "chai": "4.3.10",
    "eslint": "8.55.0",
    "eslint-plugin-prettier": "5.0.1",
    "mocha": "10.2.0",
    "prettier": "3.1.0",
    "sass": "1.69.5",
    "sinon": "17.0.1",
    "ts-mocha": "10.0.0",
    "tslib": "2.6.2",
    "typescript": "5.3.2"
  },
  "resolutions": {
    "dayjs": "1.11.10"
  },
  "scripts": {
    "compile": "yarn workspaces foreach --all -p run compile",
    "clean-ts": "yarn workspaces foreach --all -p run clean-ts",
    "deploy": "yarn workspaces foreach --all -p run deploy",
    "develop": "yarn compile && cd vue && yarn dev",
    "dev": "cd vue && yarn dev",
    "start": "node start",
    "test": "yarn compile && cd batchjobs && yarn test && cd ../shared && yarn test",
    "lint": "yarn workspaces foreach --all -p run eslint",
    "clean-all-modules": "rm -rf node_modules backend/node_modules batchjobs/node_modules shared/node_modules vue/node_modules"
  },
  "packageManager": "yarn@4.0.2"
}
