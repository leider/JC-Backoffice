    "cookie-parser": "1.4.7",
    "express": "5.1.0",
    "jc-shared": "workspace:^",
    "jsonwebtoken": "9.0.2",
    "morgan": "1.10.0",
    "multiparty": "4.2.3",
    "nodemailer": "6.10.1",
    "passport": "0.7.0",
    "passport-http-bearer": "1.0.1",
    "passport-jwt": "4.0.1",
    "pug": "3.0.3",
    "puppeteer": "23.6.1",
    "sharp": "0.33.5",
    "superagent": "10.2.0",
    "winston": "3.17.0"
