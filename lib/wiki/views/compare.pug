extends ../../../views/layout

block title
  | #{pageName} : #{subdir} - Vergleichen

block content
  .col-12
    .page-header
      .btn-group.pull-right
        a.btn.btn-light(href='javascript:history.back()', type='cancel') Zur√ºck
      h2 Vergleich von "#{pageName}"<br>
        small in Wiki '#{subdir}'

  table.compare.table.table-bordered.table-hover.table-sm
    each line in lines
      tr
        td.ln #{line.ldln}
        td.ln #{line.rdln}
        td(class=line.class) #{line.text}
